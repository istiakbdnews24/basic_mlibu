#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//bin/sh" ]; then
  exec "$basedir//bin/sh" -e "$basedir/../@quintype/create-malibu-app/bin/create-malibu-app" "$@"
else 
  exec /bin/sh -e "$basedir/../@quintype/create-malibu-app/bin/create-malibu-app" "$@"
fi
